{
  "version": 2,
  "name": "intellirename-webapp",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "backend/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ],
  "env": {
    "DEEPSEEK_API_KEY": "@deepseek-api-key",
    "GEMINI_API_KEY": "@gemini-api-key"
  },
  "functions": {
    "backend/main.py": {
      "runtime": "python3.9"
    }
  },
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite"
}